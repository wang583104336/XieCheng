<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!--面向服务开发-->
    <!--1、应用名称-->
    <dubbo:application name="userprovider"></dubbo:application>
    <!--2、注册中心-->
    <dubbo:registry protocol="zookeeper" address="${dubbo.zkurl}"></dubbo:registry>
    <!--3、发布协议-->
    <dubbo:protocol name="dubbo" port="20880"></dubbo:protocol>
    <!--4、发布服务-->
    <dubbo:service interface="com.edu.xc.service.user.UserService" ref="userProvider" retries="0"></dubbo:service>
</beans>